<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
  xsi:schemaLocation="
                      http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd
                      http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
  <property name="autoIncrement"
    value="true" />
  <property name="now"
    value="now()" />
  <property name="SUNETID_LENGTH"
    value="8" />
  <property name="ENUM_LENGTH"
    value="32" />
  <property name="SMALL_TEXT_LENGTH"
    value="64" />
  <property name="MEDIUM_TEXT_LENGTH"
    value="255" />
  <property name="LARGE_TEXT_LENGTH"
    value="4000" />

  <!-- Include database changes -->
  <include relativeToChangelogFile="true"
    file="./0.0.x/data.migrations.xml" />
  <include relativeToChangelogFile="true"
    file="./0.0.x/seed.data.xml" />

 
</databaseChangeLog>
